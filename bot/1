from cryptography.fernet import Fernet


print("Choose Account Type: ")
print("1. Standard Plan")
print("2. Premium Plan")
print("           ")

plan = int(input())

key = Fernet.generate_key()
f = Fernet(key)

if plan == 1:
    account_plan = "STPLAN".encode()
    cipher = f.encrypt(account_plan)
    print(cipher)
    print(key)


